{
  "name": "tabii-app",
  "version": "1.0.0",
  "description": "Tabii App with Vue.js frontend and Strapi backend",
  "scripts": {
    "frontend:dev": "cd frontend && pnpm run dev",
    "frontend:build": "cd frontend && pnpm run build",
    "frontend:preview": "cd frontend && pnpm run preview",
    "backend:dev": "cd backend && pnpm run develop",
    "backend:start": "cd backend && pnpm run start",
    "backend:build": "cd backend && pnpm run build",
    "dev": "concurrently \"pnpm run frontend:dev\" \"pnpm run backend:dev\"",
    "install:all": "pnpm install && cd frontend && pnpm install && cd ../backend && pnpm install",
    "build:all": "pnpm run frontend:build && pnpm run backend:build"
  },
  "keywords": ["vue", "strapi", "fullstack"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
